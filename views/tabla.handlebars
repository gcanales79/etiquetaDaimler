
<div class="container">
    <form>
        <div class="form-group">
            <label for="cantidadAbuscar">Últmos Registros a Buscar</label>
            <input type="number" autofocus="autofocus" class="form-control" id="cantidadAbuscar">
            <small id="emailHelp" class="form-text text-muted">ültimos Registros a Buscar</small>
        </div>
        <button type="submit" class="btn btn-primary" id="buscarRegistros">Submit</button>
        
    </form>
    <br>


    <div class="table-responsive">
        <table class="table table-striped table-bordered hover" style="width:100%" id="tablaConsulta" >
            <thead>
                <tr>
                    <th scope="col">Serial</th>
                    <th scope="col">Repetida</th>
                    <th scope="col">Uso</th>
                    <th scope="col">Fecha Creacion</th>
                    <th scope="col">Escaneo GP-12</th>
                    <th scope="col">Escaneo Polonia</th>
                </tr>
            </thead>
            <tbdody>
                {{#each etiqueta}}
                <tr>
                    <th scope="row">{{this.serial}}</th>
                    <td>{{#if this.repetida}}Si{{else}}No{{/if}}</td>
                    <td>{{this.uso_etiqueta}}</td>
                    <td>{{formatDate this.createdAt}}</td>
                    <td>{{formatDate this.fecha_gp12}}</td>
                    <td>{{formatDate this.fechapolonia}}</td>
                </tr>
                {{/each}}
            </tbdody>
        </table>
    </div>
</div>
</br>
</br>
</br>

<script src="https://code.jquery.com/jquery.js"></script>
<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.js"></script>
