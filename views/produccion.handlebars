<div class="container">
    <div class="row">
        <div class="col-sm-12 col-lg">
            <h2>Etiqueta a Revisar</h2>
            <form>
                <div class="input-group input-group-lg">
                    <div class="input-group-prepend">
                        <span class="input-group-text">Serial</span>
                    </div>
                    <input type="number" class="form-control" autofocus="autofocus" id="serialEtiqueta" aria-label="Sizing example input"
                        aria-describedby="inputGroup-sizing-lg">
                </div>
                <br>
                <button class="btn btn-primary" type="submit" id="submit">Revisar</button>
            </form>
            <br>
            <h1>Últimas 6 Etiquetas</h1>
            <div class="table-responsive">
                <table class="table table-striped table-sm">
                    <thead>
                        <tr>
                            <th scope="col">Serial</th>
                            <th scope="col">Repetida</th>
                            <th scope="col">Fecha Creacion</th>
                        </tr>
                    </thead>
                    <tbody id="tablaDe6">

                    </tbody>
                </table>
            </div>
            <h1>Producción Por Hora</h1>
            <div class="table-responsive">
                <table class="table table-striped table-sm">
                    <thead>
                        <tr>
                            <th scope="col">Hora</th>
                            <th scope="col">Produccion</th>

                        </tr>
                    </thead>
                    <tbody id="tablaHora">

                    </tbody>
                </table>
            </div>
            <br>

            <div id="graficaTurno">
                <h1>Producción Por Turno</h1>
                <div id="loadingTurno" class="loader"></div>
                <canvas id="myChart"></canvas>
            </div>
            <br>
            <br>
              <div id="graficaSemana">
                <h1>Producción Por Semana</h1>
                <div id="loadingSemana" class="loader"></div>
                <canvas id="myChart2"></canvas>
                
            </div>
            <br>
            <br>

        </div>
        <div class="col-sm-12 col-lg">
            <h2>Resultado</h2>
            <div class="row justify-content-center" id="Resultado">


            </div>

        </div>
    </div>


</div>
<br>



<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.js"></script>
<script src="https://code.jquery.com/jquery.js"></script>
<script src="https://cdn.jsdelivr.net/momentjs/2.12.0/moment.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.21/moment-timezone.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.21/moment-timezone-with-data-2012-2022.min.js"></script>
<script src="js/label.js"></script>